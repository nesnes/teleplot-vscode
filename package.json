{
	"name": "teleplot",
	"description": "Teleplot - Ridiculously-simple telemetry viewer.",
	"version": "1.0.8",
	"publisher": "alexnesnes",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/nesnes/teleplot-vscode"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:teleplot.start",
		"onWebviewPanel:teleplot"
	],
	"main": "./out/extension.js",
	"icon": "media/images/logo-color.png",
	"contributes": {
		"commands": [
			{
				"command": "teleplot.start",
				"title": "Start teleplot session",
				"category": "teleplot",
				"icon": {
					"light": "media/images/logo-color.svg",
					"dark": "media/images/logo-color.svg"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"rebuild:native-modules": "node ./node_modules/electron-rebuild/lib/cli.js --version 1.4.13",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"dgram": "^1.0.1",
		"node-usb-native": "^0.0.20",
		"serialport": "^10.4.0",
		"vsce": "^2.9.2"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.47.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"electron-prebuilt": "1.4.13",
		"electron-rebuild": "^3.2.3",
		"eslint": "^7.21.0",
		"typescript": "^4.4.3"
	}
}
